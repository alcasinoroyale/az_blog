<div class="login">
<%= form_for @user, url: login_path do |f| %>
  <%= f.label :id, "Username" %><br>
  <%= f.collection_select :username, User.all, :username, :username %>
  <br>
  <br>
  <%= f.label :password %><br><%= f.password_field :password %><br><br>
  <%= f.submit "Log In" %>
<% end %>
</div>
