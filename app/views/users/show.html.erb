<h2><%= @user.username %></h2>

<% if @user.id == current_user.id %>
  <button class="edit"><%= link_to 'Edit Profile', edit_user_path(@user) %></button>
<% end %>

<h3><%= @user.username %>'s Posts</h3>

<ol>
<% @user.posts.each do |post| %>
    <li><%= link_to post.title, post_path(post) %></li>
<% end %>
</ol>
