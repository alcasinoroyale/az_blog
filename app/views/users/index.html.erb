<div class="all-users">
  <h1>AZ Blog Users</h1>
  <table id="users-list" align="center" width="60%">
    <tr>
      <th><h3>Username</h3></th>
      <th><h3>Posts</h3></th>
      <th><h3>Joined</h3></th>
    </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.username, user %></td>
      <td><%= user.posts.count %></td>
      <td><%= user.created_at.strftime("%B %d, %Y") %></td>
    </tr>
  <% end %>
</table>
</div>
